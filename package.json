		"@types/uuid": "^9.0.0",
		"apollo-datasource-rest": "^3.7.0",
		"apollo-server": "^3.11.1",
		"class-validator": "^0.14.0",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
